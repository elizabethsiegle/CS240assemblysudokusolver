class node:
		def __init__(self):
			self.data = None
			self.next = None
		
class linkedList:
		def __init__(self):
			self.current = None
		
		def add_node(self, data):
			new_node = Node()
			new_node.data = data
			new_node.next = self.current
			self.current = new_node
			
l = linkedList()
l.add_node(42)
l.add_node(0)
l.add_node(1)
l.add_node(17)
l.add_node(-2)